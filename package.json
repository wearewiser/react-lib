{
  "name": "{{pkg}}",
  "version": "0.0.0",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./data": {
      "import": "./lib/data/index.js",
      "types": "./lib/data/index.d.ts"
    },
    "./models": {
      "import": "./lib/models/index.js",
      "types": "./lib/models/index.d.ts"
    },
    "./ui": {
      "import": "./lib/ui/index.js",
      "types": "./lib/ui/index.d.ts"
    },
    "./ui/components": {
      "import": "./lib/ui/components/index.js",
      "types": "./lib/ui/components/index.d.ts"
    },
    "./ui/pages": {
      "import": "./lib/ui/pages/index.js",
      "types": "./lib/ui/pages/index.d.ts"
    },
    "./hooks": {
      "import": "./lib/hooks/index.js",
      "types": "./lib/hooks/index.d.ts"
    },
    "./utils": {
      "import": "./lib/utils/index.js",
      "types": "./lib/utils/index.d.ts"
    },
    "./styles": "./lib/index.css",
    "./static/*": "./lib/static/*",
    "./images/*": "./lib/static/images/*",
    "./files/*": "./lib/static/files/*"
  },
  "bin": {
    "copy-static": "./copy-static.js"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "scripts": {
    "postinstall": "copy-static 2>/dev/null || exit 0",
    "test": "jest test --passWithNoTests",
    "test:watch": "jest test --passWithNoTests --watch",
    "test:coverage": "jest test --passWithNoTests --coverage",
    "build": "npm run clean && node ./node_modules/.bin/webpack --mode production",
    "tsc": "npm run clean && node node_modules/typescript/bin/tsc --p tsconfig.json",
    "clean": "rm -rf lib",
    "lint": "./node_modules/.bin/eslint . --ext ts",
    "docs": "node node_modules/typedoc/bin/typedoc --out docs/ src/ && echo '*\n!.gitignore' > docs/.gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "ini": "^5.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tsnode-di": "0.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-links": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/nextjs": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "chai": "^4.3.7",
    "classnames": "^2.5.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.36.0",
    "eslint-plugin-storybook": "^0.11.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "mocha": "^10.2.0",
    "nodemon": "^1.12.1",
    "npm-run-parallel": "^0.6.0",
    "nyc": "^15.1.0",
    "sass": "^1.85.0",
    "sass-loader": "^16.0.5",
    "storybook": "^8.5.8",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  }
}
